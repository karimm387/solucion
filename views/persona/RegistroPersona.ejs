<div class="container">
  <div id="page-wrapper">
  	<div class="row">
  		<br />
  		<div class="col-lg-12">
  			<div class="panel with-nav-tabs panel-primary">
  				<div class="panel-heading clearfix">
  					<div class="pull-left">
  						<label>Registro de Persona</label>
  					</div>
  				</div>
  				<div class="panel-body">
  					<ul class="nav nav-tabs" id="tablatabs">
  						<li class="active"><a data-toggle="tab" href="#tab1"> <em>Lista</em></a></li>
  						<li class=""><a data-toggle="tab" href="#tab2"> <em>Registro</em></a></li>
  					</ul>
  					<div class="tab-content">
  						<div id="tab1" class="tab-pane fade in active">
  							<br />
  							<div class="form-horizontal">
  							</div>
  							<fieldset>
  								<table class="table table-striped table-bordered table-list">
  									<thead>
  										<tr>
  											<th>Nombre</th>
                        <th>Login</th>
                        <th>Curp</th>
  											<th>Email</th>
  											<th>Telefono</th>
  											<th>Estado</th>
  										</tr>
  									</thead>
  									<tbody id="tblPersonas"></tbody>
  								</table>
  							</fieldset>
  						</div>
  						<div id="tab2" class="tab-pane fade">
  							<br />
                <form class="form-horizontal" action="" method="POST" accept-charset="utf-8">
                  <div class="form-group">
									  <div class="col-sm-2">
                      <label>Nombre</label>
                    </div>
                    <div class="col-sm-6">
                      <input type="string" id="nombre"  name="nombre"
                      class="form-control">
                    </div>
                  </div>
                  <div class="form-group">
                     <div class="col-sm-2">
                      <label>Apellido Paterno </label>
                    </div>
                    <div class="col-sm-6">
                      <input type="string" name="apPaterno" id="apPaterno" class="form-control">
                    </div>
                  </div>
                  <div class="form-group">
                     <div class="col-sm-2">
                      <label>Apellido Materno </label>
                    </div>
                    <div class="col-sm-6">
                      <input type="string" name="apMaterno" id="apMaterno" class="form-control">
                    </div>
                  </div>
                  <div class="form-group">
                     <div class="col-sm-2">
                      <label>CURP</label>
                    </div>
                    <div class="col-sm-6">
                      <input type="string" name="curp" id="curp" class="form-control">
                    </div>
                  </div>
                  <div class="form-group">
                    <div class="col-sm-2">
                      <label for="fechaNacimiento">Fecha de Nacimiento</label>
                    </div>
                    <div class="col-sm-4">
                      <div class="input-group date fecha"
            						data-date-format="dd-mm-yyyy">
            						<input name="fechaNacimiento" id="fechaNacimiento"
            							class="form-control input-sm" readonly="true"></input>
            						<span class="input-group-addon"><span
            							class="glyphicon glyphicon-calendar"></span></span>
            					</div>
                    </div>
                  </div>
                  <div class="form-group">
                     <div class="col-sm-2">
                      <label>Tel&eacute;fono</label>
                    </div>
                    <div class="col-sm-6">
                      <input type="string" name="telefono" id="telefono" class="form-control">
                    </div>
                  </div>
                  <div class="form-group">
                     <div class="col-sm-2">
                      <label>Colonia</label>
                    </div>
                    <div class="col-sm-6">
                      <input type="string" name="colonia" id="colonia" class="form-control">
                    </div>
                  </div>
                  <div class="form-group">
                     <div class="col-sm-2">
                      <label>Calle </label>
                    </div>
                    <div class="col-sm-4">
                      <input type="string" name="calle" id="calle" class="form-control">
                    </div>
                    <div class="col-sm-2">
                     <label>N&uacute;mero </label>
                   </div>
                   <div class="col-sm-4">
                     <input type="string" name="numero" id="numero" class="form-control">
                   </div>
                  </div>
                  <div class="form-group">
                     <div class="col-sm-2">
                      <label>Usuario</label>
                    </div>
                    <div class="col-sm-6">
                        <input type="string" name="login" id="login"  class="form-control">
                    </div>
                  </div>
                  <div class="form-group">
                     <div class="col-sm-2">
                          <label>Contrase&ntilde;a</label>
                    </div>
                    <div class="col-sm-6">
                          <input type="password" name="password"  class="form-control">
                    </div>
                  </div>
                  <div class="form-group">
                    <div class="col-sm-2">
                         <label>Confirma Contrase&ntilde;a</label>
                   </div>
                   <div class="col-sm-6">
                           <input type="password" name="passwordConfirmation"  class="form-control">
                   </div>
                  </div>
                  <div class="form-group">
                     <div class="col-sm-2">
                      <label>Email</label>
                    </div>
                    <div class="col-sm-6">
                      <div class="input-group">
                        <span class="input-group-addon" id="basic-addon1">@</span>
                        <input type="text" class="form-control" placeholder="Username" aria-describedby="basic-addon1">
                      </div>
                    </div>
                  </div>
                  <br>
                  <input type="hidden" name="rol" value="rol" readonly="true">
                  <input type="hidden" name="id" value="id" readonly="true">
                  <input type="submit" value="Guardar" class="btn btn-success"/>
                </form>
  						</div>
  					</div>
  				</div>
  			</div>
  		</div>
  	</div>
  </div>



</div>

<script type="text/javascript" src="/sl/pr/RegistroPersonaJS.js"></script>
